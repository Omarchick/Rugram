{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AuthOptions": {
    "Issuer": "RugramAuth",
    "Audience": "Rugram",
    "AccessTokenLifetimeInMinutes": 2,
    "RefreshTokenLifetimeInHours": 168
  },
  "Cache": {
    "SlidingExpirationForRefreshTokenInMinutes": 5
  },
  "MailConfirmationToken": {
    "LifetimeInHours": 24
  },
  "EmailConfig": {
    "Sender": "alinaagro@inbox.ru",
    "SenderName": "RugramES"
  },
  "SmtpSettings": {
    "Port": 587,
    "SmtpAddress": "smtp.mail.ru"
  },
  "Domain": {
    "AppUrl": "http://Rugram:"
  }
}
